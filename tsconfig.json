{
  "extends": "./node_modules/electron-webpack/tsconfig-base.json",
  "compilerOptions": {
    "jsx": "react",
    "noImplicitAny": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es6",
    "allowJs": true
  },
  "exclude": [
    "dist/**/*",
    "release/**/*",
    "webpack.main.ext.js",
    "babel.config.js",
    "__tests__/**/*"
  ]
}
